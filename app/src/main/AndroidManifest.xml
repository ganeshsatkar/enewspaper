<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="grdp.ganeshinc.newspaper">

    <uses-sdk tools:ignore="GradleOverrides" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission
        android:name="android.permission.READ_PHONE_STATE"
        tools:node="remove" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission
        android:name="android.permission.GET_ACCOUNTS"
        tools:node="remove" />
    <uses-permission
        android:name="android.permission.READ_PROFILE"
        tools:node="remove" />
    <uses-permission
        android:name="android.permission.READ_CONTACTS"
        tools:node="remove" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">


        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />




        <activity android:name="grdp.ganeshinc.newspaper.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>


        <activity
            android:name="com.google.android.gms.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"
            android:theme="@android:style/Theme.Translucent" />


               <activity
            android:name=".Sakal"
            android:label="@string/m1"
            android:parentActivityName="grdp.ganeshinc.newspaper.MainActivity"
            tools:ignore="UnusedAttribute" />


        <activity
            android:name=".Lokmat"
            android:label="@string/m2"
            android:parentActivityName="grdp.ganeshinc.newspaper.MainActivity"
            tools:ignore="UnusedAttribute" />


        <activity
            android:name=".Mata"
            android:label="@string/m3"
            android:parentActivityName="grdp.ganeshinc.newspaper.MainActivity"
            tools:ignore="UnusedAttribute" />

        <activity
            android:name=".Loksatta"
            android:label="@string/m4"
            android:parentActivityName="grdp.ganeshinc.newspaper.MainActivity"
            tools:ignore="UnusedAttribute" />


        <activity
            android:name=".Pudhari"
            android:label="@string/m5"
            android:parentActivityName="grdp.ganeshinc.newspaper.MainActivity"
            tools:ignore="UnusedAttribute" />


        <activity
            android:name=".Punyanagari"
            android:label="@string/m6"
            android:parentActivityName="grdp.ganeshinc.newspaper.MainActivity"
            tools:ignore="UnusedAttribute" />


        <activity
            android:name=".Samana"
            android:label="@string/m7"
            android:parentActivityName="grdp.ganeshinc.newspaper.MainActivity"
            tools:ignore="UnusedAttribute" />

        <activity
            android:name=".Prahar"
            android:label="@string/m8"
            android:parentActivityName="grdp.ganeshinc.newspaper.MainActivity"
            tools:ignore="UnusedAttribute" />
        <activity
            android:name=".Divyamarathi"
            android:label="@string/m9"
            android:parentActivityName="grdp.ganeshinc.newspaper.MainActivity"
            tools:ignore="UnusedAttribute" />


        <activity
            android:name=".Tarunbharat"
            android:label="@string/m10"
            android:parentActivityName="grdp.ganeshinc.newspaper.MainActivity"
            tools:ignore="UnusedAttribute" />


        <activity
            android:name=".Newspaper"
            android:label="@string/m11"
            android:parentActivityName="grdp.ganeshinc.newspaper.MainActivity"
            tools:ignore="UnusedAttribute" />



        <activity android:name=".Splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <service android:name=".MessagingService"
            tools:ignore="ExportedService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <meta-data
            android:name="com.google.firebase.messaging.default_notification_icon"
            android:resource="@drawable/logo" />
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_color"
            android:resource="@color/colorAccent" />

    </application>

</manifest>